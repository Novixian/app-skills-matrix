{
  "name": "app-skills-matrix",
  "version": "1.0.0",
  "description": "OSS tool to manage employees skills",
  "main": "backend/index.js",
  "engines": {
    "node": "8",
    "npm": "5"
  },
  "scripts": {
    "start-dev": "eval $(cat .env) nodemon",
    "start": "node backend/index.js",
    "base": "node $npm_package_main",
    "lint:test": "tslint ./test/**/*.ts",
    "lint:frontend": "tslint ./frontend/**/*.ts*",
    "lint": "tslint ./**/*.ts ./**/*.tsx",
    "lint-fix": "tslint --fix ./**/*.ts*",
    "test": "eval $(cat .env.test) mocha --recursive --compilers ts:ts-node/register,tsx:ts-node/register",
    "build": "webpack --debug",
    "watch": "NODE_ENV=local webpack --watch",
    "precommit": "npm run lint",
    "prepush": "eval $(cat .env.test) npm run test",
    "postinstall": "webpack",
    "ensure-local-data": "NODE_ENV=local node scripts/ensure-local-data",
    "setup-dokku-deploy": "git remote add dokku dokku@dokku.management.tescloud.com:skills-matrix",
    "deploy": "git pull --rebase && git push dokku master"
  },
  "repository": {
    "url": "git@github.com:tes/app-skills-matrix.git",
    "type": "git"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/bluebird": "^3.5.8",
    "@types/chai": "^4.0.4",
    "@types/express": "^4.0.37",
    "@types/jsonwebtoken": "^7.2.3",
    "@types/mocha": "^2.2.42",
    "@types/mongodb": "^2.2.11",
    "@types/node": "^8.0.27",
    "@types/node-uuid": "0.0.28",
    "@types/ramda": "^0.24.10",
    "@types/react": "^15.6.2",
    "@types/react-bootstrap": "0.0.52",
    "@types/react-copy-to-clipboard": "^4.2.3",
    "@types/react-router": "^3.0.13",
    "@types/supertest": "^2.0.3",
    "awesome-typescript-loader": "^3.2.3",
    "chai": "^4.1.2",
    "css-loader": "^0.26.1",
    "husky": "^0.14.3",
    "mocha": "^3.5.0",
    "node-sass": "^4.5.0",
    "nodemon": "^1.12.0",
    "sass-loader": "^4.1.1",
    "snazzy": "^5.0.0",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.13.1",
    "supertest": "^2.0.1",
    "webpack": "^2.1.0-beta.25"
  },
  "dependencies": {
    "authom": "^0.4.36",
    "aws-sdk": "^2.110.0",
    "axios": "^0.16.2",
    "bluebird": "^3.4.7",
    "body-parser": "^1.16.1",
    "bootstrap": "^3.3.7",
    "composable-middleware": "^0.3.0",
    "cookie-parser": "^1.4.3",
    "debug": "^2.2.0",
    "express": "^4.15.4",
    "express-handlebars": "^3.0.0",
    "express-validation": "^1.0.2",
    "file-loader": "^0.11.2",
    "google-auth-library": "^0.10.0",
    "jsonwebtoken": "^7.4.3",
    "joi": "^10.6.0",
    "keymirror": "^0.1.1",
    "moment": "^2.17.1",
    "mongodb": "^2.2.31",
    "morgan": "^1.7.0",
    "pify": "^3.0.0",
    "ramda": "^0.24.1",
    "react": "^15.4.2",
    "react-bootstrap": "^0.31.3",
    "react-copy-to-clipboard": "^5.0.0",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.6",
    "react-router": "^3.0.2",
    "react-router-bootstrap": "^0.23.1",
    "react-tooltip": "^3.3.0",
    "redux": "^3.6.0",
    "redux-actions": "^1.2.1",
    "redux-devtools-extension": "^2.13.0",
    "redux-saga": "^0.14.3",
    "redux-thunk": "^2.2.0",
    "serialize-javascript": "^1.3.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.7.0",
    "tslint-config-airbnb": "^5.2.1",
