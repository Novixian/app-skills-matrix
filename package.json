{
  "name": "app-skills-matrix",
  "version": "1.0.0",
  "description": "OSS tool to manage employees skills",
  "main": "backend/index.js",
  "engines": {
    "node": "6",
    "npm": "yarn"
  },
  "scripts": {
    "start-dev": "eval $(cat .env) nodemon",
    "start": "node backend/index.js",
    "debug": "nodemon --debug",
    "base": "node $npm_package_main",
    "lint": "eslint --ext .js ./backend ./test ./frontend",
    "lint-fix": "eslint --fix --ext .js ./backend ./test ./frontend",
    "test": "eval $(cat .env.test) mocha --recursive --require babel-register",
    "build": "webpack --debug",
    "watch": "NODE_ENV=local webpack --watch",
    "precommit": "yarn lint",
    "prepush": "eval $(cat .env.test) yarn test",
    "postinstall": "webpack",
    "ensure-local-data": "NODE_ENV=local node scripts/ensure-local-data",
    "setup-dokku-deploy": "git remote add dokku dokku@dokku.management.tescloud.com:skills-matrix",
    "deploy": "git pull --rebase && git push dokku master"
  },
  "repository": {
    "url": "git@github.com:tes/app-skills-matrix.git",
    "type": "git"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "chai": "^4.0.0-canary.1",
    "css-loader": "^0.26.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-react": "^7.1.0",
    "husky": "^0.14.3",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.0",
    "nodemon": "^1.11.0",
    "sass-loader": "^4.1.1",
    "snazzy": "^5.0.0",
    "style-loader": "^0.13.1",
    "supertest": "^2.0.1",
    "webpack": "^2.1.0-beta.25"
  },
  "dependencies": {
    "authom": "^0.4.36",
    "aws-sdk": "^2.32.0",
    "axios": "^0.15.3",
    "babel-core": "^6.22.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "bluebird": "^3.4.7",
    "body-parser": "^1.16.1",
    "bootstrap": "^3.3.7",
    "cookie-parser": "^1.4.3",
    "debug": "^2.2.0",
    "express": "^4.14.0",
    "express-handlebars": "^3.0.0",
    "file-loader": "^0.10.0",
    "google-auth-library": "0.9.8",
    "jsonwebtoken": "^7.1.9",
    "keymirror": "^0.1.1",
    "moment": "^2.17.1",
    "mongodb": "^2.2.12",
    "morgan": "^1.7.0",
    "normalizr": "^3.2.2",
    "pify": "^2.3.0",
    "ramda": "^0.24.1",
    "react": "^15.4.2",
    "react-bootstrap": "^0.30.8",
    "react-copy-to-clipboard": "^5.0.0",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-bootstrap": "^0.23.1",
    "redux": "^3.6.0",
    "redux-actions": "^1.2.1",
    "redux-devtools-extension": "^2.13.0",
    "redux-saga": "^0.14.3",
    "redux-thunk": "^2.2.0",
    "serialize-javascript": "^1.3.0",
    "url-loader": "^0.5.7"
  }
}
